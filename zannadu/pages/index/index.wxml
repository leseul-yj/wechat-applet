<view class="binner">
	<swiper indicator-dots="true" indicator-color="rgba(255, 255, 255,0.5)" indicator-active-color="#fff" autoplay="true" circular="true" previous-margin="20" next-margin="20">
		<swiper-item wx:for="{{binnerList}}" wx:key="index">
			<image mode="HeightFix" src="{{item.url}}"></image>
		</swiper-item>
	</swiper>

</view>

<!-- 预定行程 -->
<view class="itinerary">
  <view class="txtTitle">预定行程</view>
  <view class="itinerary_nav">
    <view class="itinerary_item" wx:for="{{itineraryList}}" wx:key="index">
      <navigator>
        <image class="itinerary_icon" src="{{item.url}}" mode="HeightFix"></image>
        <view>{{item.title}}</view>
      </navigator>
    </view>
  </view>
</view>
<!-- 限时活动 -->
<view>
<view class="txtTitle">限时活动</view>
</view>